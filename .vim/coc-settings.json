{
  "signature.enable": 100,
  "signature.triggerSignatureWait": 100,
  "diagnostic.enable": true,
  "diagnostic.level": "error",
  "suggest.autoTrigger": "always",
  "suggest.timeout": 1000,
  "phpls.trace.server": "verbose",
  "coc.preferences.rootPatterns": ["composer.json","composer.lock",".idea","package.json",".git",".hg",".projections.json"],
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "list.normalMappings": {
    "<C-c>": "do:refresh"
  },
  "list.insertMappings": {
    "<C-c>": "do:refresh",
    "<C-o>": "call:CocAction('showSignatureHelp')",
    "<C-p>": "action.triggerParameterHints"
  },
  "cursors.cancelKey": "<C-c>",
  "cursors.nextKey": "<C-j>",
  "cursors.previousKey": "<C-k>",
  "coc.preferences.formatOnSaveFiletypes": [
    "go"
  ],
  "intelephense.licenceKey": "",
  "intelephense.telemetry.enabled": false,
  "intelephense.completion.triggerParameterHints": false,
  "intelephense.format.enable": false,
  "intelephense.trace.server": "off",
